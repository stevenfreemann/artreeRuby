<!DOCTYPE html>
<html>
  <head>
    <title>Artree</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <div class="head">
      <div>Logo</div>
      <div>iconos</div>
      <div>Menu</div>
    </div>
    <%= yield %>

    <footer>
      <div>
        <div>Logo</div>
        <div>menus</div>
        <div>Redes</div>
      </div>
      <div>Copyright</div>
    </footer>
  </body>
</html>
